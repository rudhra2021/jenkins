pipeline{
  agent any
    stages{
      stage("InitialStage"){
        steps{
        echo"this is initial stage"
        }
      }
      stage("ParallelStages"){
        parallel{
         stage("P1"){
           steps{
            echo"paralleljobp1"
            sleep 15
            }
          }
        
          stage("P2"){
            steps{
            echo"paralleljobp2"
            sleep 15
            }
          }
        }
      }
    }
 }
