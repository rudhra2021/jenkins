pipeline{
  agent any
    stages{
      stage("initial"){
       
        parallel{
         stage("P1"){
           steps{
            echo"paralleljobp1"
            sleep 15
            }
          }
        
          stage("P2"){
            steps{
            echo"paralleljobp2"
            sleep 15
            }
          }
        }
      }
    }
 }
